{
  "name": "{{cookiecutter.project_slug}}",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20.17.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:orval && tsc -b && vite build",
    "knip": "knip --fix --allow-remove-files",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prune": "eslint --fix --prune-suppressions",
    "style:fix": "prettier --write .",
    "fix": "npm run lint:fix && npm run style:fix",
    "lint:check": "eslint .",
    "style:check": "prettier --check .",
    "check": "npm run lint:check && npm run style:check",
    "preview": "vite preview",
    "generate:orval": "orval",
    "prune-suppressions": "eslint --prune-suppressions",
    "precommit": "npm build && npm run fix",
    "prepush": "npm run check"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.84.1",
    "aws-amplify": "^6.15.5",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.522.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-pwa": "^1.0.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "knip": "^5.79.0",
    "orval": "^7.11.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
